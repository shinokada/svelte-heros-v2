{
	"name": "svelte-heros-v2",
	"version": "0.3.0",
	"description": "Hero icon v2 components for Svelte",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/svelte-heros-v2/issues",
	"homepage": "https://github.com/shinokada/svelte-heros-v2",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "husky install",
		"release": "npm run build && changeset publish",
		"version-packages": "changeset version && npm run format"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.6",
		"@changesets/cli": "^2.24.4",
		"@playwright/test": "^1.25.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.472",
		"@sveltejs/package": "1.0.0-next.1",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.0",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.16",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2",
		"vite": "^3.1.0"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"Hero icons",
		"svg icons",
		"icons"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/svelte-heros-v2"
	}
}
